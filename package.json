    "documentation": "^8.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^1.0.0-rc.14",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "semantic-release": "^15.6.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
